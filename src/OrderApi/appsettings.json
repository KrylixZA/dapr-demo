{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ActorConfig": {
    "actorIdleTimeout": 10,
    "actorScanInterval": 10,
    "drainOngoingCallTimeout": 30,
    "drainRebalancedActors": true,
    "remindersStoragePartitions": 2,
    "reentrancyConfig": {
      "enabled": true,
      "maxStackDepth": 5
    }
  }
}
